    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Build",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal", 
            "args": ["build","--inputs","../../linux/init","--dataset","./data/linux_init"]
        },
        {
            "name": "Python Debugger: Train",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal", 
            "args": ["train","--dataset","./data/linux_init","--model","./models/base.xyz"]
        },
        {
            "name": "Python Debugger: Test",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal", 
            "args": ["test","--model","./models/base.xyz"]
        },
        {
            "name": "Python Debugger: Deploy",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal", 
            "args": ["deploy","--model", "./models.base.xyz", "--refresh"]
        }

    ]
}